{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/weather/weather.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${metap}/weather/weather.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "${workspaceFolder}/weather/weather.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "appId": "myapp",
            "appPort": 5000,
            "label": "daprd-debug",
            "type": "daprd",
            "dependsOn": "build"
        },
        {
            "appId": "myapp",
            "label": "daprd-down",
            "type": "daprd-down"
        },
        {
            "type": "go",
            "label": "go: test package",
            "command": "test",
            "args": [
                "${"temp/input.html"}"
            ],
            "problemMatcher": [
                "$go"
            ],
            "group": "test",
            "detail": "cd /workspaces/kubernetes-in-codespaces; go test ${fileDirname}"
        }
    ]
}